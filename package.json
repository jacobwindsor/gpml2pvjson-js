{
  "name": "gpml2pvjson",
  "version": "1.0.6",
  "description": "Convert GPML (XML) to and from pvjson (JSON)",
  "main": "index.js",
  "scripts": {
    "build": "browserify ./index.js -o ./test/lib/gpml2pvjson.bundle.js",
    "test": "npm run test:1x && npm run test:2x",
    "test:unit": "npm run test:1x:unit",
    "test:1x": "npm run test:1x:unit",
    "test:1x:unit": "echo \"The tests in the 1x branch currently fail in Node.js but might work in the browser. TODO: set this up to run using PhantomJS instead of JSDom.\"",
    "test.browseronly:1x:unit": "mocha test/spec/api.js --timeout=16000",
    "test:2x": "echo \"The tests for streaming are broken or outdated in this 1x branch. And the tests in the 2x branch do not work with the dependencies installed as specified by the 1x package.json. Use the tests in the 2x branch and the dependencies as specified by the 2x package.json.\""
  },
  "keywords": [
    "gpml",
    "pathway",
    "biology",
    "bioinformatics"
  ],
  "author": "Anders Riutta",
  "license": "Apache-2.0",
  "dependencies": {
    "async": "~0.7.0",
    "biopax2json": "git://github.com/wikipathways/biopax2json.git#dcd9a06864abe2da20002a0b4d3512d006ef2f71",
    "cheerio": "^0.16.0",
    "csv": "~0.3.7",
    "csv-streamify": "~1.0.0",
    "d3": "~3.4.6",
    "he": "^0.5.0",
    "iconv-lite": "^0.4.13",
    "lodash": "^2.4.2",
    "rgbcolor": "0.0.3",
    "rx-extra": "ariutta/rx-extra#ec215ee2e833721ae038e06c3dd46751e71aa633",
    "tower-strcase": "git://github.com/tower/strcase.git",
    "uuid": "~1.4.1",
    "xmlhttprequest": "~1.6.0"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "chai": "^3.5.0",
    "colors": "~0.6.2",
    "deep-diff": "^0.3.4",
    "figlet": "~1.0.8",
    "gulp": "~3.8.1",
    "gulp-browserify": "~0.5.0",
    "highland": "^3.0.0-beta.3",
    "hyperquest": "^1.2.0",
    "jquery": "^1.12.3",
    "jsdom": "^8.3.0",
    "loggin": "^3.0.1",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "pretty-data": "~0.40.0",
    "pretty-error": "^2.0.0",
    "sinon": "^1.17.3"
  },
  "repository": {
    "type": "git",
